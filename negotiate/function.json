{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
        "type": "SignalRConnectionInfo",
        "name": "connectionInfo",
        "hubName": "serverless",
        "connectionStringSetting": "SIGNALR_CONNECTION_STRING",
        "direction": "in"
    }
  ]
}